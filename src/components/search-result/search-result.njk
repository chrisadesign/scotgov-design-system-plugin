<{{ params.element }} class="ds_search-result {%- if params.promoted %}  ds_search-result--promoted{%- endif %}">
    {%- if params.promoted %}
    <div class="ds_search-result--promoted-content">
    <header class="ds_search-result--promoted-title">{{ params.promoted }}</header>
    {%- endif %}
    <{{ params.headingLevel }} class="ds_search-result__title">
        <a href="{{ params.link }}">{{ params.heading }}</a>
    </{{ params.headingLevel }}>
    {%- if params.image %}
    <div class="ds_search-result__has-media">
        <div class="ds_search-result__media-wrapper">
            <div class="ds_search-result__media  ds_aspect-box  ds_aspect-box--square">
                <a href="{{ params.link }}" tabindex="-1">
                    <img alt="" aria-hidden="true" class="ds_aspect-box__inner" src="{{ params.image }}" width="656" height="369"/>
                </a>
            </div>
        </div>
        <div>
    {%- endif %}
    {%- if params.subHeading %}
    <{{ params.subHeadingLevel }} class="ds_search-result__sub-title">{{ params.subHeading }}</{{ params.subHeadingLevel }}>
    {%- endif %}
    {%- if params.content %}
    <p class="ds_search-result__summary">{{ params.content | safe }}</p>
    {%- endif %}
    {%- if params.metadata %}
    {% render '@metadata', params.metadata %}
    {%- endif %}
    {%- if params.parent %}
    <dl class="ds_search-result__context">
        <dt class="ds_search-result__context-key">{{ params.parent.heading }}</dt>
        <dd class="ds_search-result__context-value"><a href="{{ params.parent.link }}">{{ params.parent.linkLabel }}</a></dd>
    </dl>
    {%- endif %}
    {%- if params.subLinks %}
    <{{ params.subLinksHeadingLevel }} class="ds_search-result__sublinks-title">Included within this result</{{ params.subLinksHeadingLevel }}>
    <ul class="ds_search-result__sublinks  ds_contents-nav__list">
        {%- for subLink in params.subLinks -%}
        <li class="ds_contents-nav__item">
            <a class="ds_contents-nav__link" href="{{ subLink.link }}">
                {{ subLink.linkLabel }}
            </a>
        </li>
        {%-  endfor %} 
    </ul>
    {%- endif %}
    {%- if params.image %}
        </div>
    </div>
    {%- endif %}
    {%- if params.promoted %}
    </div>
    {%- endif %}
</{{ params.element }}>