<{{ params.element }} class="ds_search-result">
    <{{ params.headingLevel }} class="ds_search-result__title">
        <a class="ds_search-result__link" href="{{ params.link }}">{{ params.heading }}</a>
    </{{ params.headingLevel }}>
    {%- if params.image %}
    <div class="ds_search-result__has-media">
        <div class="ds_search-result__media">
            <div class="ds_aspect-box  ds_aspect-box--square">
                <img alt="" aria-hidden="true" class="ds_aspect-box__inner" src="{{ params.image }}" width="656" height="369"/>
            </div>
        </div>
    {%- endif %}
    {%- if params.content %}
    <p class="ds_search-result__summary">{{ params.content }}</p>
    {%- endif %}
    {%- if params.image %}
    </div>
    {%- endif %}
    {%- if params.metadata %}
    {% render '@metadata', params.metadata %}
    {%- endif %}
    {%- if params.parent %}
    <dl class="ds_search-result__context">
        <dt class="ds_search-result__context-key">{{ params.parent.heading }}</dt>
        <dd class="ds_search-result__context-value"><a href="{{ params.parent.link }}">{{ params.parent.linkLabel }}</a></dd>
    </dl>
    {%- endif %}
    {%- if params.subLinks %}
    <{{ params.subLinksHeadingLevel }} class="ds_search-result__sublinks-title">Included within this result</{{ params.subLinksHeadingLevel }}>
    <ul class="ds_search-result__sublinks  ds_contents-nav__list">
        {%- for subLink in params.subLinks -%}
        <li class="ds_contents-nav__item">
            <a class="ds_contents-nav__link" href="{{ subLink.link }}">
                {{ subLink.linkLabel }}
            </a>
        </li>
        {%-  endfor %} 
    </ul>
    {%- endif %}
</{{ params.element }}>